{
  "version": 2,
  "framework": "docusaurus",
  "public": true,
  "builds": [
    {
      "src": "frontend_book/**/*",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "build"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "frontend_book/build/$1",
      "headers": {
        "Cache-Control": "public, max-age=31536000, immutable"
      }
    }
  ]
}