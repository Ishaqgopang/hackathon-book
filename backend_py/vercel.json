{
  "version": 2,
  "functions": {
    "api/index.py": {
      "runtime": "python3.9"
    },
    "api/books/index.py": {
      "runtime": "python3.9"
    },
    "api/books/[id].py": {
      "runtime": "python3.9"
    }
  },
  "routes": [
